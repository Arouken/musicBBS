<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bbs.dao.CategoryDao">

<!-- 	查询分区表 -->
	<select id="selectCategoryList"  resultType="com.bbs.pojo.Category">
		select * from category
	</select>
	
<!-- 	添加分区 -->
	<insert id="addCategory" parameterType="com.bbs.pojo.Category">
	    insert into category (userID,categoryName,categoryTxt,categoryCreatDay,categoryImg) 
	    values(#{userID},#{categoryName},#{categoryTxt},#{categoryCreatDay},#{categoryImg})
	</insert>
	
</mapper>